                  {{if selectedNewAwardeeOrg.address.stateCode selectedNewAwardeeOrg.address.stateCode ''}}
                  {{if (eq "US" selectedNewAwardeeOrg.address.countryCode) (format-postal-code selectedNewAwardeeOrg.address.postalCode) selectedNewAwardeeOrg.address.postalCode}}

                {{/if}}
              </span><br/>
              <span data-test-change-awardee-org-new-country>{{if selectedNewAwardeeOrg selectedNewAwardeeOrg.address.countryCode '' }}</span>
            </p>
          {{/if}}

        </div>
        <div class="modal-footer">
