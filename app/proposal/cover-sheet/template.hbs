          {{#if-permission 'proposal.data.modify'}}
            <a class="padding-left-20 padding-top-10" tabindex="0" {{action "addIACountry"}} class="{{internationalActyCountriesDisabledClass}}" disabled={{internationalActyCountriesDisabled}} title={{internationalActyCountriesDisabledText}} data-test-add-international-activities-link><i class="fa fa-plus margin-top-10"></i> Add Additional Country</a>
          {{/if-permission}}
        {{/if}}

