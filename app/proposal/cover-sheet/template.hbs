                        <i class="fa fa-asterisk" aria-hidden="true"></i>Country {{add index 1}}.
                </label>
                <div class="display-inline-block">
                  <span class="display-block">
                          <select id={{concat "international-activities-country-select-" (add index 1)}} data-test-international-activities-country-select class={{if selectedCountry.intlCountryError 'error form-control' 'form-control' }} onchange={{action (action "selectIACountry" index) value="target.value"}} disabled={{viewOnly}}>

                            {{#if (eq "" selectedCountry.intlCountryCode)}}
                              <option value="" selected>Select Country</option>
                            {{else}}
                              <option value="">Select Country</option>
                            {{/if}}

                            {{#each sortedInternationalCountries as |country|}}
                              {{#if (or (eq selectedCountry.intlCountryCode country.countryCode) (not (exists-in country internationalActyCountries)) )}}

                                {{#if (eq selectedCountry.intlCountryCode country.countryCode)}}
                                  <option value={{country.countryCode}} selected>{{country.countryName}}</option>
                                {{else}}
                                  <option value={{country.countryCode}}>{{country.countryName}}</option>
                                {{/if}}

                              {{/if}}

                            {{/each}}
                          </select>
                          {{#if-permission 'proposal.data.modify'}}
                            {{#if (gt internationalActyCountries.length 1)}}
