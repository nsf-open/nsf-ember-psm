              {{#if selectedNewAwardeeOrg.address.streetAddress2}}
                <span data-test-change-awardee-org-new-street2>{{selectedNewAwardeeOrg.address.streetAddress2}}</span><br/>
              {{/if}}
              <span data-test-change-awardee-org-new-city-state-postal-code>
                {{#if selectedNewAwardeeOrg }}

